<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MyToolbar">
        <attr name="rightButtonIcon" format="reference"/>
        <attr name="isSearchShow" format="boolean"/>
        <attr name="rightButtonText" format="string"/>
        <attr name="rightText" format="string"/>
    </declare-styleable>

    <declare-styleable name="RatioLayout">
        <attr name="ratio" format="float" />
    </declare-styleable>

    <declare-styleable name="NumberAddSubView">
        <attr name="curValue" format="integer|reference"/>
        <attr name="minValue" format="integer|reference"/>
        <attr name="maxValue" format="integer|reference"/>
        <attr name="numBackground" format="reference"/>
        <attr name="addBackgroud" format="reference"/>
        <attr name="subBackgroud" format="reference"/>
    </declare-styleable>
</resources>